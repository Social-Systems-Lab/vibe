{
    "name": "vibe-cloud-api",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "concurrently \"bun run dev:api\" \"bun run dev:oidc\"",
        "dev:api": "bun run --watch src/index.ts",
        "dev:oidc": "bun run --watch src/oidc-server.ts",
        "build:js": "bun build ./src/index.ts --outdir ./dist --target node",
        "build:types": "tsc --emitDeclarationOnly",
        "build": "bun run build:js && bun run build:types"
    },
    "dependencies": {
        "@elysiajs/cookie": "^0.8.0",
        "@elysiajs/cors": "^1.3.3",
        "@elysiajs/eden": "^1.3.2",
        "@elysiajs/jwt": "^1.3.1",
        "@koa/cors": "^5.0.0",
        "@noble/ed25519": "^2.3.0",
        "@noble/hashes": "^1.8.0",
        "@types/nano": "^7.0.0",
        "@yolk-oss/elysia-env": "^3.0.0",
        "bip39": "^3.1.0",
        "buffer": "^6.0.3",
        "elysia": "^1.3.5",
        "micro-ed25519-hdkey": "^0.1.2",
        "multibase": "^4.0.6",
        "nano": "^10.1.4",
        "oidc-provider": "^9.2.0",
        "uuid": "^9.0.1",
        "varint": "^6.0.0"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/koa__cors": "^5.0.0",
        "@types/oidc-provider": "^9.1.1",
        "@types/uuid": "^9.0.8",
        "concurrently": "^8.2.2",
        "typescript": "^5.8.3"
    }
}
