{
    "private": true,
    "name": "vibe",
    "workspaces": [
        "apps/*",
        "packages/*",
        "infra"
    ],
    "scripts": {
        "build": "pnpm -r build",
        "dev:api": "pnpm --filter vibe-cloud-api dev",
        "dev:web": "pnpm --filter vibe-web dev",
        "dev:test-app": "pnpm --filter test-app dev",
        "dev:sdk": "pnpm --filter vibe-sdk dev",
        "dev:react": "pnpm --filter vibe-react dev",
        "dev": "pnpm -r --parallel dev"
    },
    "dependencies": {
        "undici": "^7.10.0"
    },
    "devDependencies": {
        "concurrently": "^9.2.0",
        "@types/varint": "^6.0.0"
    }
}
