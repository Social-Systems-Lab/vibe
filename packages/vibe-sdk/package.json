{
    "name": "vibe-sdk",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build:js": "bun build ./src/index.ts --outdir ./dist",
        "build:types": "tsc --emitDeclarationOnly",
        "build": "bun run build:js && bun run build:types",
        "dev": "concurrently \"bun run build:js --watch\" \"bun run build:types --watch\""
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elysiajs/eden": "^1.3.2",
        "jwt-decode": "^4.0.0",
        "vibe-cloud-api": "workspace:*"
    },
    "devDependencies": {
        "elysia": "^1.3.5",
        "typescript": "5.8.3",
        "concurrently": "^8.2.2"
    }
}
