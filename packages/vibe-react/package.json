{
    "name": "@vibe/react",
    "version": "0.1.0",
    "private": true,
    "description": "React Hooks and Context Provider for Vibe SDK",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
        "dev": "tsup src/index.tsx --format esm,cjs --dts --external react --watch",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "vibe",
        "sdk",
        "react",
        "hooks"
    ],
    "author": "Vibe Team",
    "license": "MIT",
    "devDependencies": {
        "@types/react": "^18.3.20",
        "@types/react-dom": "^18.3.6",
        "eslint": "^8.57.0",
        "react": "^18.3.1",
        "react-dom": "^19",
        "tsup": "^8.2.3",
        "typescript": "^5.5.4"
    },
    "peerDependencies": {
        "@vibe/sdk": "workspace:*",
        "@vibe/shared-types": "workspace:*",
        "react": "^18.2.0"
    },
    "dependencies": {
        "@vibe/sdk": "workspace:*",
        "@vibe/shared-types": "workspace:*"
    }
}
